	Mesh = {Adult; Aged; Female; Gases; Humans; Lung; Lung Diseases; Magnetic Resonance Imaging; Male; Middle Aged; Pulmonary Ventilation; Respiration; Ventilation-Perfusion Ratio; Xenon Isotopes; Young Adult},
	Month = {Dec},
