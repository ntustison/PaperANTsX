	Mesh = {Algorithms; Atlases as Topic; Epilepsy; Hippocampus; Humans; Image Interpretation, Computer-Assisted},
	Month = {Feb},
	Number = {3},
	Pages = {2457-66},
	Pmc = {PMC2818274},
	Pmid = {19818860},
